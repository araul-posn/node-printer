{
  "name": "@araul-posn/node-printer",
  "description": "Node.js printer bindings",
  "version": "0.7.0",
  "homepage": "https://github.com/araul-posn/node-printer",
  "author": {
    "name": "Ion Lupascu",
    "url": "http://program-support.co.uk/",
    "email": "ionlupascu@gmail.com"
  },
  "contributors": [
    "Thiago Lugli <thiagoelg@gmail.com>",
    "Eko Eryanto <ekoeryanto@gmail.com>",
    "Ad√£o Raul <adao.raul@gmail.com>"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/araul-posn/node-printer.git"
  },
  "devDependencies": {
    "nodeunit": "*",
    "prebuild": "^13.0.1",
    "prebuild-ci": "^4.0.0",
    "node-abi": "^3.70.0",
    "patch-package": "^8.0.0",
    "node-gyp": "^10.2.0"
  },
  "engines": {
    "node": ">= 20.0.0"
  },
  "scripts": {
    "install": "prebuild-install || node-gyp rebuild",
    "apply-patches": "patch-package",
    "prebuild-all": "prebuild --all --force --strip --verbose",
    "prebuild": "prebuild",
    "rebuild": "node-gyp rebuild",
    "test": "nodeunit test",
    "create-release": "gh release create v$(node -p \"require('./package.json').version\") --title \"v$(node -p \"require('./package.json').version\")\" --notes \"Prebuilt binaries for node-printer\" --draft",
    "publish-release": "gh release edit v$(node -p \"require('./package.json').version\") --draft=false"
  },
  "binary": {
    "module_name": "node_printer",
    "module_path": "./lib/",
    "napi_versions": [
      6
    ],
    "host": "https://github.com/araul-posn/node-printer/releases/download/",
    "remote_path": "v{version}"
  },
  "licenses": [
    {
      "type": "BSD"
    }
  ],
  "main": "./lib/printer",
  "dependencies": {
    "node-addon-api": "^8.3.0",
    "prebuild-install": "^7.1.2"
  },
  "types": "types/index.d.ts"
}
